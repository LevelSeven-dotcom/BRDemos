@model KnowledgeHubPortal.Domain.Entities.Catagory
@*<script src="~/lib/jquery/dist/jquery.min.js"></script>*@


<h2>Create Catagory</h2>
<hr />

<form action="/Catagory/Create" method="post">

    Enter Catagory Title <br />
    <input type="text" asp-for="Name" placeholder="Enter Catagory" /><br />
    <span class="text-danger" asp-validation-for="Name"></span><br />
    Enter Description <br />
    <textarea asp-for="Description" rows="6" cols="60"></textarea><br />
    <span class="text-danger" asp-validation-for="Description"></span><br />
    <input type="submit" name="btnSubmit" value="Submit" />


</form>



@section Scripts
{

<script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
<script src="~/lib/jquery-validation-unobtrusive/dist/jquery.validate.unobtrusive.min.js"></script>
}
